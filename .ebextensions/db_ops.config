container_commands:
  01_replace_settings:
    command: "replace_settings.sh"
    leader_only: true
  02_migrate:
    command: "python manage.py migrate --noinput"
    leader_only: true
  03_collectstatic:
    command: "python manage.py collect --noinput"
    leader_only: true

option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: foursquare_search.settings